{
  "name": "serverless-prisma2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p generate:prisma watch:nexus offline",
    "predeploy": "run-s clean generate:nexus",
    "deploy": "sls deploy",
    "offline": "sls offline",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "clean": "rimraf .webpack",
    "watch:nexus": "tsnd src/schema",
    "db:save": "dotenv -- prisma migrate save --experimental",
    "db:migrate": "dotenv -- prisma migrate up --experimental"
  },
  "license": "MIT",
  "dependencies": {
    "@nexus/schema": "0.13.1",
    "@prisma/cli": "2.0.0-beta.4",
    "@prisma/client": "2.0.0-beta.4",
    "@types/node-fetch": "2.5.6",
    "apollo-server-lambda": "^2.12.0",
    "graphql": "14.6.0",
    "nexus-prisma": "0.12.0"
  },
  "devDependencies": {
    "@types/node": "13.13.1",
    "apollo": "^2.27.3",
    "copy-webpack-plugin": "^5.1.1",
    "dotenv-cli": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.69.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.1.4",
    "serverless-webpack": "^5.3.2",
    "ts-loader": "^7.0.3",
    "ts-node": "8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
